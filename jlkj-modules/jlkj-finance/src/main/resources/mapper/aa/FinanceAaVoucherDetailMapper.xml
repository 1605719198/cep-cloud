    <sql id="selectFinanceAaVoucherDetailVo">
        select id, company_id, voucher_id, voucher_date, voucher_no, srlno, drcr, group_acct_id, acct_id, acct_code, acct_name, cal_ida, cal_codea, cal_namea, cal_idb, cal_codeb, cal_nameb, cal_idc, cal_codec, cal_namec, cal_idd, cal_coded, cal_named, crcy_unit, qty_frnAmt, ntAmt, srl_desc from finance_aa_voucher_detail
    </sql>

    <select id="selectFinanceAaVoucherDetailList" parameterType="com.jlkj.finance.aa.domain.FinanceAaVoucherDetail" resultMap="FinanceAaVoucherDetailResult">
        SELECT DISTINCT
        a.* ,
        b.cal_type_codeA,
        b.cal_type_codeB,
        b.cal_type_codeC,
        b.cal_type_codeD
        FROM
        finance_aa_voucher_detail a
        LEFT JOIN finance_aa_acctcode b ON b.acct_id = a.acct_id and
        b.acct_code=a.acct_code
        <where>  
            <if test="companyId != null  and companyId != ''"> and a.company_id = #{companyId}</if>
            <if test="voucherNo != null  and voucherNo != ''"> and a.voucher_no = #{voucherNo}</if>
        </where>
        order by a.voucher_no
    </select>

    <select id="selectFinanceAaVoucherBatchExecutionList" parameterType="com.jlkj.finance.aa.domain.FinanceAaVoucherDetail" resultMap="FinanceAaVoucherDetailResult">
        <include refid="selectFinanceAaVoucherDetailVo"/>
        <where>
            <if test="companyId != null  and companyId != ''"> and company_id = #{companyId}</if>
            <if test="voucherNo != null  and voucherNo != ''"> and voucher_no = #{voucherNo}</if>
        </where>
        order by voucher_no
    </select>
    
    <select id="selectFinanceAaVoucherDetailById" parameterType="String" resultMap="FinanceAaVoucherDetailResult">
