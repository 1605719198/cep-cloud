        where id = #{id}
    </select>
    <select id="selectFinanceAaVoucherList" resultType="com.jlkj.finance.aa.domain.FinanceAaVoucherDetail">
        select
        DISTINCT
        a.voucher_no,a.voucher_date,a.apId,a.voucher_desc,a.dr_amt,a.cr_amt,a.pastuser_name,a.status
        from  finance_aa_voucher a
        left join finance_aa_voucher_detail b on b.voucher_no = a.voucher_no AND  b.company_id = a.company_id

        <where>
            <if test="companyId != null  and companyId != ''"> and b.company_id = #{companyId}</if>
            <if test="startDate != '' and endDate != ''"> and b.voucher_date between #{startDate} and #{endDate}</if>
            <if test="voucherNoStart != '' and voucherNoEnd != ''"> and b.voucher_no between #{voucherNoStart} and #{voucherNoEnd}</if>
            <if test="acctNameStart != '' and acctNameEnd != ''"> and b.acct_name between #{acctNameStart} and #{acctNameEnd}</if>
            <if test="calNameaStart != '' and calNameaEnd != ''"> and b.cal_namea between #{calNameaStart} and #{calNameaEnd}</if>
            <if test="calNamebStart != '' and calNamebEnd != ''"> and b.cal_nameb between #{calNamebStart} and #{calNamebEnd}</if>
            <if test="qtyFrnamtStart != '' and qtyFrnamtEnd != ''"> and b.qty_frnAmt between #{qtyFrnamtStart} and #{qtyFrnamtEnd}</if>
        </where>
        order by b.voucher_no
    </select>

